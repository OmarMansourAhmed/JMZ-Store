package com.example.jana;

public class Mouse extends Accessories{
    double Sensitivity;

    Mouse(){}
    Mouse(String name,double price,int instock,String connectionType,boolean ledRGB,double Sensitivity){
        super(name,price,instock,connectionType,ledRGB);
        this.Sensitivity=Sensitivity;
    }

    public double getSensitivity() {
        return Sensitivity;
    }

    public void setSensitivity(double sensitivity) {
        Sensitivity = sensitivity;
    }
}
